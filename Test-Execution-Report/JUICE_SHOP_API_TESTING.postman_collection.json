		{
			"name": "User Address Details",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Response contains required fields\", function () {\r",
							"    let jsonData = pm.response.json();\r",
							"    \r",
							"    // Check if 'status' field exists and is 'success'\r",
							"    pm.expect(jsonData).to.have.property(\"status\", \"success\");\r",
							"\r",
							"    // Ensure 'data' array exists and is not empty\r",
							"    pm.expect(jsonData).to.have.property(\"data\").that.is.an(\"array\").that.is.not.empty;\r",
							"\r",
							"    // Validate first object in data array (assuming there's at least one)\r",
							"    let address = jsonData.data[0];\r",
							"    \r",
							"    pm.expect(address).to.have.property(\"id\");\r",
							"    pm.expect(address).to.have.property(\"UserId\");\r",
							"    pm.expect(address).to.have.property(\"fullName\");\r",
							"    pm.expect(address).to.have.property(\"mobileNum\");\r",
							"    pm.expect(address).to.have.property(\"zipCode\");\r",
							"    pm.expect(address).to.have.property(\"streetAddress\");\r",
							"    pm.expect(address).to.have.property(\"city\");\r",
							"    pm.expect(address).to.have.property(\"state\");\r",
							"    pm.expect(address).to.have.property(\"country\");\r",
							"    pm.expect(address).to.have.property(\"createdAt\");\r",
							"    pm.expect(address).to.have.property(\"updatedAt\");\r",
							"});\r",
							"\r",
							"pm.test(\"Response contains submitted address\", function () {\r",
							"    let jsonData = pm.response.json();\r",
							"    let addressList = jsonData.data;\r",
							"\r",
							"    let found = addressList.some(address => address.fullName === \"Liza Akter\" && address.mobileNum === 1713005933);\r",
							"    pm.expect(found).to.be.true;\r",
							"});\r",
							""
						],
