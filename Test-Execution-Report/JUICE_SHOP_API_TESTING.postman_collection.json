							"});\r",
							"\r",
							"// Check if 'message' exists before asserting\r",
							"pm.test(\"Response contains required fields\", function () {\r",
							"    pm.expect(jsonData).to.be.an(\"object\");\r",
							"    pm.expect(jsonData).to.have.property(\"status\", \"success\");\r",
							"\r",
							"    if (jsonData.hasOwnProperty(\"message\")) {\r",
							"        pm.expect(jsonData).to.have.property(\"message\");\r",
							"    } else {\r",
							"        console.warn(\"⚠️ Warning: 'message' property is missing from the response\");\r",
							"    }\r",
							"});\r",
